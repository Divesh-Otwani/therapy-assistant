<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="haverford.therapy_assistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:replace="android:icon">
        <activity android:name=".Homepage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.exercise.Exercises"
            android:label="@string/title_activity_exercises" />
        <activity
            android:name=".activity.exercise.SelectExercise"
            android:label="Select Exercise" />
        <activity
            android:name=".activity.resource.ResourceHome"
            android:label="@string/title_activity_resources" />
        <activity
            android:name=".activity.resource.Resource"
            android:label="@string/title_activity_cbt_resources" />
        <activity
            android:name=".activity.exercise.DoExercise"
            android:label="@string/title_activity_do_exercise"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".activity.exercise.QuestionAnswer"
            android:label="QuestionAnswer" />
        <activity
            android:name=".activity.exercise.NoQuestion"
            android:label="@string/title_activity_no_question"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
    </application>

</manifest>